generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

// smaple schema
model User {
  id       String  @id @default(auto()) @map("_id") @db.ObjectId
  email    String?
  username String?
  password String
  avatar   String?
  bio      String?
}
